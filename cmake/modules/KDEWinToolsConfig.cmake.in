# - Config file for the KDEWin-tools package
# It defines the following variables
#  KDEWIN_TOOLS_VERSION      - the version of the kdewin tools package as a number
#  KDEWIN_TOOLS_VERSION_STR  - the version of the kdewin tools package as a string
#  KDEWIN_TOOLS_MINGW_W32    - if this library has been compiled with the 32bit compiler of mingw-w64 project

get_filename_component(KDEWIN_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Our library dependencies (contains definitions for IMPORTED targets)
include("${KDEWIN_CMAKE_DIR}/KDEWinToolsTargets.cmake")

set(KDEWIN_TOOLS_MINGW_W32 @MINGW_W32@)

set(KDEWIN_TOOLS_VERSION @KDEWIN_VERSION@)
set(KDEWIN_TOOLS_VERSION_STR @KDEWIN_VERSION_STR@)
